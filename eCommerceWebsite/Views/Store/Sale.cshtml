@model eCommerceWebsite.Models.CategoryViewModel

@{
    ViewBag.Title = "Sale";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <br />
    <h2>Sales</h2>
    <hr />
</div>
<div class="row">
    <div style="float: right;">
        @Html.Partial("PaginationPartial", Model)
    </div>
</div>
<div class="row">
    @foreach (eCommerceWebsite.Models.ProductResult product in Model.Products)
    {
        @Html.Partial("ProductPartial", product)
    }
</div>
